<div class="fake-container-fluid mb-3">
  <div class="row" id="events">
    <% 5.times do %>
      <div class="col-12">
        <div class="mb-4">
          <div class="row block no-gutters align-items-stretch bg-white">
            <div class="col-lg-12 order-lg-1">
              <table style="width: 100%; height: 100%">
                <tr>
                  <td style="vertical-align: top">
                    <div class="row justify-content-between">
                      <div class="col ml-1 p-lg-3">
                        <h4 class="block-title flicker" style="width: 50%; background: rgba(0,0,0,0.1)">&nbsp;</h4>
                        <div style="font-size: 1rem">
                          <% 3.times do %>
                            <div class="flicker mb-1" style="width: 33%; background: rgba(0,0,0,0.1)">&nbsp;</div>
                          <% end %>
                        </div>
                      </div>
                    </div>
                  </td>
                </tr>
              </table>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
