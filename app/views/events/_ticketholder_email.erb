<% form_tag "/events/#{@event.id}/orders/#{@order.id}/ticketholders/#{ticket.id}/email", id: 'ticketholders', class: 'mb-0' do %>
  <%= hidden_field_tag :success, value: success %>
  <%= email_field_tag :email, value: ticket.email, onchange: "showSavedTooltipAndSubmit(this);", class: 'form-control', placeholder: "Email" %>
<% end %>
