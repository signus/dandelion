<div class="modal-header">
  <h3 class="modal-title"><a href="/accounts/<%= membership.account_id %>"><%= membership.account.name %></a> in <%= membership.gathering.name %></h3>
  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<div class="modal-body">
  <%= partial :'gatherings/membership', locals: { membership: membership } %>
</div>
