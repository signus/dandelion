<% form_tag '', method: 'get', class: 'submitOnChange searchForm form-inline mb-3' do %>
  <div class="form-group">
    <label>Search feedback</label>
    <%= text_field_tag :q, class: 'form-control', value: params[:q] %>
  </div>
  <%= checkbox 'Hide 5 stars' %>
  <%= hidden_field_tag :search, value: 1 %>
  <%= submit_tag 'Search', class: 'btn btn-primary' %>
<% end %>

<%= partial :'event_feedbacks/event_feedbacks_table', locals: { event_feedbacks: @event_feedbacks, event_name: true, organisation_name: true } %>
